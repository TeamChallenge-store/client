{
  "name": "team-challenge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://teamchallenge-store.github.io/client",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --ignore-path .gitignore --report-unused-disable-directives",
    "format": "prettier . --ignore-path .prettierignore --write",
    "fix-style": "npm run format & npm run lint",
    "deploy": "gh-pages -d dist",
    "predeploy": "npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.9",
    "@mui/lab": "^5.0.0-alpha.165",
    "@mui/material": "^5.15.9",
    "@reduxjs/toolkit": "^2.0.1",
    "classnames": "^2.5.1",
    "formik": "^2.4.5",
    "gh-pages": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.3",
    "redux-persist": "^6.0.0",
    "swiper": "^11.1.0",
    "use-debounce": "^10.0.0",
    "uuid": "^9.0.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@mate-academy/eslint-config-react-typescript": "^1.0.13",
    "@types/classnames": "^2.3.1",
    "@types/node": "^20.11.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "prettier": "3.2.4",
    "sass": "^1.70.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run fix-style --exit-on-error"
  }
}
